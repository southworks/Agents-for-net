{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/vdevPreview/MicrosoftTeams.schema.json",
  "manifestVersion": "devPreview",
  "version": "1.0.0",
  "id": "${{AAD_APP_CLIENT_ID}}",
  "packageName": "com.microsoft.agents.streaming",
  "developer": {
    "name": "Microsoft, Inc.",
    "websiteUrl": "https://example.azurewebsites.net",
    "privacyUrl": "https://example.azurewebsites.net/privacy",
    "termsOfUseUrl": "https://example.azurewebsites.net/termsofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "AgentSDK Streaming responses",
    "full": "AgentSDK Streaming OpenAI responses"
  },
  "description": {
    "short": "Sample demonstrating AgentSDK + OpenAI to deminstrate streaming responses",
    "full": "Sample demonstrating AgentSDK + OpenAI to deminstrate streaming responses"
  },
  "accentColor": "#FFFFFF",
  "copilotAgents": {
    "customEngineAgents": [
      {
        "id": "${{AAD_APP_CLIENT_ID}}",
        "type": "bot"
      }
    ]
  },
  "bots": [
    {
      "botId": "${{AAD_APP_CLIENT_ID}}",
      "scopes": [
        "personal"
      ],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "<<BOT_DOMAIN>>"
  ]
}